{
  "Serilog": {
    "MinimumLevel": "Error",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": { "path": "Logs/log.txt", "rollingInterval": "Day", "retainedFileCountLimit": 2  }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName", "WithThreadId"],
    "Properties": {
      "Application": "PrintService"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Redis": "localhost"
  },
  "Printer": {
    "OutletId": 1,
    "PrinterId": 1,
    "Name": "Printer 1"
  }
}
